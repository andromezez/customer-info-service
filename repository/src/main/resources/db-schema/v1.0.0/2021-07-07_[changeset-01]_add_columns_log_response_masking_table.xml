<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet  id="add_columns_log_response_masking_table"  author="bayu"  failOnError="true">
        <addColumn tableName="masking" >
            <column name="at_log" type="BOOLEAN" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
            <column name="at_response" type="BOOLEAN" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
        </addColumn>
        <rollback>
            <dropColumn tableName="masking">
                <column name="at_log"/>
                <column name="at_response"/>
            </dropColumn>
            <output  target="STDOUT">rollback drop columns at_log, at_response from table masking</output>
        </rollback>
    </changeSet>
</databaseChangeLog>